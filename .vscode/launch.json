{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to Air Debugger",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1",
            "preLaunchTask": "Wait for Delve (port 2345)",
            "showLog": true,
            "trace": "verbose",
            "logOutput": "rpc"
        },
        {
            "name": "Go Run",
            "type": "go",
            "request": "launch",
            "args": ["serve"],
            "mode": "auto",
            "program": "${workspaceFolder}"
        }
    ]
}